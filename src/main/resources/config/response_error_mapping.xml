<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>

 <worksheet name="Application">
	    <section startRow="0" endRow="0">
		</section>    
        <loop startRow="1" endRow="2000" items="parserData.applicationErrorConstants" var="responseError" varType="com.ahana.api.system.security.error.ResponseError">
            <section startRow="1" endRow="1">            
            	<mapping cell="A3">responseError.errorCode</mapping>
		        <mapping cell="B3">responseError.shortMessage</mapping>
		        <mapping cell="C3">responseError.longMessage</mapping>
			</section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
        
    <worksheet name="Validation">
	    <section startRow="0" endRow="0">
		</section>    
        <loop startRow="1" endRow="2000" items="parserData.validationErrorConstants" var="responseError" varType="com.ahana.api.system.security.error.ResponseError">
            <section startRow="1" endRow="1">
		        <mapping cell="A3">responseError.errorCode</mapping>
		        <mapping cell="B3">responseError.shortMessage</mapping>
		        <mapping cell="C3">responseError.longMessage</mapping>
			</section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0">STOP</cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
    
    <worksheet name="Security">
	    <section startRow="0" endRow="0">
		</section>    
